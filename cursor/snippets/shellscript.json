{
  "Major comment heading": {
    "prefix": "**",
    "body": [
      "# =======================================",
      "# $1",
      "# ======================================= $2"
    ]
  },
  "Minor comment heading": {
    "prefix": "*",
    "body": [
      "# ======",
      "# $1",
      "# ====== $2"
    ]
  }
}
